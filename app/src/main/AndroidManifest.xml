<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.akilliajans.KriptoTakipv1"
    android:exported="true">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE">

    </uses-permission>


    <application
        android:allowBackup="true"

        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"




        android:theme="@style/Theme.KriptoTracke">
        <activity android:name="com.akilliajans.KriptoTakipv1.view.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN"
                    android:colorMode="hdr"
                    android:screenOrientation="portrait"


                    tools:targetApi="o" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.akilliajans.KriptoTakipv1.model.Elementals"
            android:screenOrientation="fullSensor"
            />






    </application>

</manifest>